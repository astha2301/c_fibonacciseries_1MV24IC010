#include<stdio.h>
int main ()
{
    int sum, i, n, a, b;
    a=0;
    b=1;
    sum=0;
    printf ("This is the fibonacci series.\n");
    printf ("Enter the number: ");
    scanf ("%d",&n);
    for (i=1;i<=n;i++)
    {
       printf ("%d ", a);     
       sum=a+b;
       a=b;
       b=sum;
    }
    return 0;
}
